            border: silver;
            border-radius: 2vh 2vh 2vh 2vh;
            outline: none;
            width: 60%;
            color: rgb(187, 143, 0);
            font-size: 6vh;
            box-shadow: 2vh 0vh 8vh rgb(0, 0, 0);
            transition: 0.7s;
            font-weight: bold;
        }

        .btnp:hover {
            background: linear-gradient(rgb(255, 153, 0) 40%, rgb(255, 187, 0) 100%);
            color: rgb(0, 0, 0);
            box-shadow: 0vh 0vh 8vh rgba(255, 255, 255, 0.377);
