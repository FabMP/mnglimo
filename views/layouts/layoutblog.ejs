            flex-grow: 3;
        }

        hr {
            background-color: rgba(255, 207, 74, 0.918);
        }

        .menu {
            display: inline;
            padding: 1vh;
        }

        .nav-link {
            color: white;
            font-size: 4vh;
            margin-left: 2vh;
            text-decoration: none;
        }

        .nav-link:hover {
