        .container-fluid {
            padding: 1rem;
        }

        #logotipo {
            width: clamp(2.75em, 1em + 4vw, 3.5em);
        }

        .container-fluid {
            flex-grow: 3;
        }

        hr {
            width: 90%;
            background-color: rgba(255, 207, 74, 0.918);
        }

        #logotipo {
            width: 40vh;
        }

        .menu {
            display: inline;
            padding: 1vh;
        }

        .nav-link {
            color: white;
            font-size: 4vh;
            margin-left: 2vh;
            text-decoration: none;
        }

        .nav-link:hover {
            color: rgba(255, 207, 74, 0.918);
            transition: 0.35s;
            text-decoration: none;
            text-shadow: 0vh 0vh 1vh rgba(243, 183, 19, 0.589);
        }

        .navbar-toggler {
            margin-left: 28vh;
        }

        .fadein {
            animation: fade-in 1.5s;
        }

        #fleetpic {
            width: 100%;
        }

        .bi {
            height: 4vh;
            width: 4vh;
            color: rgba(255, 207, 74, 0.918);
        }

        #limoback {
            width: 90%;
            border-radius: 1vh 1vh 1vh 1vh;
            box-shadow: 0vh 0vh 10vh black;
            border: solid 0.5vh rgba(243, 183, 19, 0.589);
        }

        .hide {
            display: none;
        }

        .hidden-label {
            display: none;
        }

        @keyframes fade-in {
            from {
                margin-left: 5%;
            }

            to {
                margin-left: 0%;
            }

            0% {
                opacity: 0;
            }

            100% {
                opacity: 1;
            }
        }

        @media (max-width: 992px) {

            .titulo {
                font-size: 6.5vh;
            }

            .marca {
                font-size: 6vh;
            }

            .btnp {
                font-size: 4.5vh;
            }

            .send {
                font-size: 4.5vh;
            }

            .contact-info {
                margin-top: 5vh;
            }

            .form-control {
                margin-top: 2vh;
                width: 130%;
                height: 5vh;
            }

            #fleetpic {
                width: 80%;
            }

            #fleetpicdiv {
                display: flex;
                justify-content: center;
            }

            .brand {
                display: flex;
                justify-content: center;
            }

        }

        @media (max-width: 605px) {
            #main {
                height: 70h;
                width: 100%;
            }

