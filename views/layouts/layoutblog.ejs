        .btnp {
            background-color: rgba(243, 183, 19, 0.918);
            border: rgba(243, 183, 19, 0.918);
            border-radius: 2vh 2vh 2vh 2vh;
            outline: none;
            font-weight: bold;
            width: 60%;
            color: rgba(0, 0, 0, 0.918);
            font-size: 6vh;
            box-shadow: 2vh 0vh 8vh rgb(0, 0, 0);
            transition: 0.3s;
        }

        .btnp:hover {
            background-color: rgb(0, 0, 0);
            color: rgba(243, 183, 19, 0.918);
            box-shadow: 2vh 0vh 8vh rgba(247, 210, 0, 0.377);
        }

